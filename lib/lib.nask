#!/bin/sh
progname=`basename $0`
targets="{traceroute,bgp}.nask.router"

if [ "$progname" == "lib.nask" -a "$1" == "targets" ]; then
	echo $targets
	exit
fi

address=$1
service=`echo $progname |cut -f 1 -d .`
[ "$service" == "traceroute" ] && service=tracert

wget -q --post-data "operacja=$service&router=RR-1.core.nask.pl&dana=$address" http://www.nask.pl/cgi-bin/look.pl -O - |
	sed -e's/<[^>]*>//g'

